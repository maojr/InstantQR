{
  "manifest_version": 2,

  "name": "InstantQR",
  "description": "This extension shows a QR code image for the current page url",
  "version": "1.0",

  "icons": {
    "16":"icon16.png",
    "48":"icon48.png",
    "128":"icon128.png"
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },

  "permissions": [
    "activeTab",
    "http://qr.liantu.com/"
  ]
}
